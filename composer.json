{
    "name": "aron/signagture-verification",
    "description": "接口签名验证",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Aron Yao",
            "email": "Aron.Yao@feisu.com"
        }
    ],
    "keywords": [
        "signature"
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-json": "*",
        "php": "^7.4 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Aron\\Signature\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aron\\Sigature\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aron\\Signature\\Provider\\SignatureServiceProvider"
            ],
            "aliases": {
                "Signature": "Aron\\Singature\\Facades\\Signature"
            }
        }
    }
}
